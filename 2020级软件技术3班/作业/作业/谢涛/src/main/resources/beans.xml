<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="student" class="com.TT.pojo.Student">
        <constructor-arg name="id" value="2580"/>
        <constructor-arg name="name" value="谢涛"/>
        <constructor-arg name="sex" value="男"/>
</bean>

    <bean id="employee" class="com.TT.pojo.Employee" p:id="901300" p:name="谢涛" p:sex="男" p:height="178cm" p:age="111"></bean>

</beans>
